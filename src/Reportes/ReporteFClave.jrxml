	<queryString>
		<![CDATA[SELECT
     vw_infocard.`folio` AS vw_infocard_folio,
     vw_infocard.`clave` AS vw_infocard_clave,
     convert(vw_infocard.`kg_hojas`,unsigned int) AS vw_infocard_kg_hojas,
     convert(vw_infocard.`bob_paq`,unsigned int) AS vw_infocard_bob_paq,
     vw_infocard.`concepto` AS vw_infocard_concepto,
     CONVERT(vw_infocard.`costo`,decimal(10,2)) AS vw_infocard_costo,
     CONVERT(vw_infocard.`costo`,decimal(10,2))* convert(vw_infocard.`kg_hojas`,unsigned int) AS total,
     vw_infocard.`fecha` AS vw_infocard_fecha,
     vw_infocard.`observaciones` AS vw_infocard_observaciones,
     vw_infocard.`tipo` AS vw_infocard_tipo
FROM
     `vw_infocard` vw_infocard
where  vw_infocard.`clave` = $P{clave}
ORDER BY vw_infocard.`clave`, vw_infocard.`fecha`,vw_infocard.`tipo`, vw_infocard.`folio`]]>
	</queryString>
